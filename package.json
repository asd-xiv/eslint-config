{
  "name": "@codemachiner/eslint-config",
  "version": "2.9.2",
  "description": "JavaScript ESLint rules",
  "main": "rules/frontend.js",
  "scripts": {
    "setup": "rm -f package-lock.json && rm -rf node_modules && npm i",
    "lint:js": "eslint ./rules/*",
    "lint:md": "markdownlint *.md",
    "lint": "npm run lint:js && npm run lint:md",
    "check:update": "npm-check -u",
    "prepare": "npm run lint",
    "preversion": "npm run prepare"
  },
  "homepage": "https://github.com/codemachiner/eslint-config",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codemachiner/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/codemachiner/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-rules",
    "react",
    "flow",
    "flowtype",
    "config",
    "jest",
    "javascript",
    "codemachiner"
  ],
  "author": "Andrei Dumitrescu <andrei@codemachiner.com> (https://twitter.com/andreidcm)",
  "dependencies": {},
  "devDependencies": {
    "eslint": "4.12.1",
    "eslint-plugin-compat": "2.1.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-flowtype-errors": "3.3.6",
    "eslint-plugin-html": "4.0.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.3.2",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-jsx-control-statements": "2.2.0",
    "eslint-plugin-no-inferred-method-name": "1.0.2",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-unicorn": "3.0.0",
    "markdownlint-cli": "0.5.0",
    "npm-check": "5.5.2"
  },
  "peerDependencies": {
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-no-inferred-method-name": "^1.0.2",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-unicorn": "^3.0.0"
  },
  "engines": {
    "node": ">= 6"
  },
  "license": "MIT"
}
